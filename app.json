{
    "expo": {
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#ffffff",
                "foregroundImage": "./assets/images/adaptive-icon.png"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.hashfall.RFIDWalkingCane",
            "permissions": [
                "BLUETOOTH",
                "BLUETOOTH_ADMIN",
                "BLUETOOTH_SCAN",
                "BLUETOOTH_CONNECT",
                "ACCESS_FINE_LOCATION",
                "NEARBY_WIFI_DEVICES"
            ]
        },
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "da2e1e89-b57b-43f1-8b20-36bb998fe91d"
            },
            "router": {}
        },
        "icon": "./assets/images/icon.png",
        "ios": {
            "infoPlist": {
                "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to connect to your walking cane.",
                "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to connect to your walking cane.",
                "NSLocationWhenInUseUsageDescription": "Location is required for Bluetooth scanning."
            },
            "supportsTablet": true
        },
        "name": "RFIDWalkingCane",
        "newArchEnabled": true,
        "orientation": "portrait",
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "backgroundColor": "#ffffff",
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain"
                }
            ],
            "react-native-ble-plx",
            [
                "react-native-permissions",
                {
                    "iosPermissions": ["Bluetooth"]
                }
            ]
        ],
        "scheme": "rfidwalkingcane",
        "slug": "RFIDWalkingCane",
        "userInterfaceStyle": "automatic",
        "version": "1.0.0",
        "web": {
            "bundler": "metro",
            "favicon": "./assets/images/favicon.png",
            "output": "static"
        }
    }
}

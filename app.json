{
    "expo": {
        "name": "RFIDWalkingCane",
        "slug": "RFIDWalkingCane",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "rfidwalkingcane",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "permissions": [
                "BLUETOOTH",
                "BLUETOOTH_ADMIN",
                "BLUETOOTH_SCAN",
                "BLUETOOTH_CONNECT",
                "ACCESS_FINE_LOCATION"
            ],
            "package": "com.hashfall.RFIDWalkingCane"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "react-native-ble-plx"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "da2e1e89-b57b-43f1-8b20-36bb998fe91d"
            }
        }
    }
}
